<!---isSemantcs_mapping: if 0, the mapping is only go for direct mapping
if 1: the mapping should be mapped based on semantics meaning>

<metaprogram>
<app>
	<appname>com.ffgames.racingincar</appname>
	<op>replace</op>
	<orientation>Y</orientation>
	<sensor_type>gyro</sensor_type>
	<freq>100</freq>
	<x-calibrate>0</x-calibrate>
	<y-calibrate>9</y-calibrate>
	<z-calibrate>0</z-calibrate>
	<orientation>Y</orientation>
	<sensor_type>accel</sensor_type>
	<freq>100</freq>
	<x-calibrate>0</x-calibrate>
	<y-calibrate>9</y-calibrate>
	<z-calibrate>0</z-calibrate>
	<isSemanticsMapping>1</isSemanticsMapping>
</app>
<mapping>
	<from>left</from>
	<to>swipe_left</to>
	<from>up</from>
	<to>swipe_up</to>
	<from>right</from>
	<to>swipe_right</to>
</mapping>
</metaprogram>
